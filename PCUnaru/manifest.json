{
  "manifest_version": 2,
  "name": "PCUnaru",
  "description": "ページ開くとめっちゃPCうなるんだが仕様？",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["http://osapi.dmm.com/gadgets/ifr*"],
      "js": ["index.js"],
      "all_frames": true,
      "run_at":"document_start"
    }
  ],
  "permissions":["webNavigation"]

}